alias vi="nvim"
alias py="python3"
alias r="ranger"


alias s="source ~/.zshrc"
alias c=clear

alias a="env /usr/bin/arch -arm64 /bin/zsh"
alias x="env /usr/bin/arch -x86_64 /bin/zsh"

# git alias
alias ga="git add ."
alias gc="git clone"
alias gm="git commit -a"
alias gp="git push --set-upstream origin master"


# nvim config alias
alias nci="nvim ~/.config/nvim/init.vim"
alias ncp="nvim ~/.config/nvim/plugin.vim"
